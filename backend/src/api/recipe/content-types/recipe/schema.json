{
  "kind": "collectionType",
  "collectionName": "recipes",
  "info": {
    "singularName": "recipe",
    "pluralName": "recipes",
    "displayName": "Recipe",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Kcal": {
      "type": "integer"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Likes": {
      "type": "integer",
      "default": 0
    },
    "Instructions": {
      "type": "richtext"
    },
    "Ingredients": {
      "type": "richtext"
    },
    "category_recipes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category-recipe.category-recipe",
      "inversedBy": "recipes"
    },
    "Description": {
      "type": "text"
    },
    "mediums": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::medium.medium",
      "mappedBy": "recipes"
    }
  }
}
